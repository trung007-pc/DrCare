@inherits LayoutComponentBase
@inject NavigationManager _navigationManager;
<PageTitle>Todo.TenantBlazor</PageTitle>

<MudLayout>

    <MudDrawer @bind-Open="@_leftDrawerOpen" ClipMode="DrawerClipMode.Never" Elevation="1" Variant="@DrawerVariant.Mini" Breakpoint="Breakpoint.Sm" Width="17vw" PreserveOpenState="false" OpenMiniOnHover="true" Style="background-color: #161616;">
        <MudDrawerHeader>
            <img style="margin-left: -10px" src="data:image/svg+xml;base64,"/>
        </MudDrawerHeader>


        <MudNavMenu Dense="true" Rounded="true" Margin="Margin.Dense" Style="color: #ffffff" Color="Color.Primary">
            <RadzenStack Orientation="Radzen.Orientation.Vertical" JustifyContent="JustifyContent.SpaceBetween">
                <div>
                    <MudNavLink IconColor="Color.Secondary" Href="/home" Icon="@Icons.Material.Filled.Home" ForceLoad="false">Home</MudNavLink>
                    <MudNavGroup Title="Administration" Icon="@Icons.Material.Filled.Build" IconColor="Color.Secondary">
                        <MudNavGroup Title="Identity management" Icon="@Icons.Material.Filled.Badge">
                            <MudNavLink IconColor="Color.Secondary" Href="/users" ForceLoad="false">Users</MudNavLink>
                            <MudNavLink IconColor="Color.Secondary" Href="/roles" ForceLoad="false">Roles</MudNavLink>
                            <MudNavLink IconColor="Color.Secondary" Href="/tenants" ForceLoad="false">Tenants</MudNavLink>

                        </MudNavGroup>
                        <MudNavLink Href="/Community3" Icon="@Icons.Material.Filled.Settings">Setting</MudNavLink>
                    </MudNavGroup>
                    <MudNavLink IconColor="Color.Secondary" ForceLoad="false" Href="/users" Icon="@Icons.Material.Filled.People">Community</MudNavLink>
                </div>


            </RadzenStack>

        </MudNavMenu>

  
    </MudDrawer>


    <MudDrawer @bind-Open="@_rightDrawerOpen"  Anchor="Anchor.End" ClipMode="DrawerClipMode.Never" Elevation="1" Variant="@DrawerVariant.Mini" Breakpoint="Breakpoint.Sm" Width="17vw" PreserveOpenState="false" Style="background-color: #161616;">
        <MudDrawerHeader Style="justify-content: center">
            <MudBadge Color="Color.Success" Overlap="true" Bordered="true">
                <MudAvatar Image="https://fcb-abj-pre.s3.amazonaws.com/img/jugadors/MESSI.jpg"/>
            </MudBadge>
        </MudDrawerHeader>

    </MudDrawer>
    <MudMainContent Style="padding: 15px">
        @Body
    </MudMainContent>

</MudLayout>


<MudThemeProvider/>
<MudDialogProvider/>
<MudSnackbarProvider/>
<RadzenDialog/>
<RadzenNotification/>
<RadzenContextMenu/>
<RadzenTooltip/>

@code {
    bool _leftDrawerOpen = false;
    bool _rightDrawerOpen = false;

    void NavigationTo(string route)
    {
        _navigationManager.NavigateTo(route);
    }

}